
<div class="container p-3">
    <div class="row pt-4">

        <div class="col-6">
            <h1 class="text-primary">Company List</h1>
        </div>
        <div class="col-6 text-end">

            <a class=" btn btn-primary" asp-Controller="Company" asp-action="Upsert">
                <i class="bi bi-plus-circle"></i> Create new Company
            </a>
        </div>
    </div>
    <br />

    <table class="table table-bordered table-striped text-primary" id="tblData" style="width:100%">
        <thead>
            <tr>
                <th>Name</th>
                <th>Address</th>
                <th>City</th>
                <th>State</th>
                <th>Contact</th>
               <th></th>
            </tr>
        </thead>

    </table>
</div>

@section Scripts{
    <script src="~/js/company.js"></script>
}
