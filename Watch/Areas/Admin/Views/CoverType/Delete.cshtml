@model CoverType

<form method="post" asp-action="DeletePost">
    <input asp-for="Id"  hidden/>
    <div class="border p-3 mt-5" >
        <div class="row pb-3">
            <h3 class="text-primary p-2">Delete CoverType</h3>
        </div>
        <div class="mb-3">
            <label asp-for="Name"></label>
            <input asp-for="Name" disabled class="form-control" />
            <br/>
            </div>
        <div>
            <button type="submit" class="btn btn-warning" style="width:150px" >Delete </button>
            <a asp-controller="CoverType" asp-action="Index" class="btn btn-secondary" style="width:150px">Cancle</a>
        </div>
    </div>

</form>

@section Scripts{
    @{
        <partial name="_ValidationScriptsPartial" />
    }
}
